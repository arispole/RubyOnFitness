<div class="card-body">
<h5  class="card-title">LOG IN</h5>
<p class="card-text">


<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :nome %><br />
    <%= f.email_field :cognome, autofocus: true, autocomplete: "nome" %>
  </div>
  
  <div class="field">
    <%= f.label :cognome %><br />
    <%= f.email_field :cognome, autofocus: true, autocomplete: "cognome" %>
  </div>

  <div class="field">
    <%= f.label : %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>


  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "current-password" %>
  </div>

  <% if devise_mapping.rememberable? %>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end %>

  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
<% end %>
